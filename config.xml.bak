<?xml version="1.0" encoding="UTF-8" ?>
<widget xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0" version="1.1.10" id="com.br.jmtecnologiabh.appdemo"
    >

    <name>App Demo</name>

    <description>
    App de demonstração
    </description>

    <author href="http://jmtecnologiabh.com.br"  
        email="contato@jmtecnologiabh.com.br"> JM Tecnologia
    </author>

    <icon src="icon.png" />
    <splash src="splash.png" />
	<preference name="stay-in-webview" value="true" />

    <access origin="*" browserOnly="true" subdomains="true" />

    <content src="index.html" />

	<allow-navigation href="*" />
	<!-- The above is equivalent to these three declarations -->
	<allow-navigation href="http://*/*" />
	<allow-navigation href="https://*/*" />
	<allow-navigation href="data:*" />
	
		<!-- Allow SMS links to open messaging app -->
	<allow-intent href="sms:*" />

	<!-- Allow tel: links to open the dialer -->
	<allow-intent href="tel:*" />

	<!-- Allow geo: links to open maps -->
	<allow-intent href="geo:*" />

	<!-- Allow all unrecognized URLs to open installed apps
		 *NOT RECOMMENDED* -->
	<allow-intent href="*" />
	
    <gap:plugin name="cordova-plugin-whitelist" source="npm" version="~1" />

    <preference name="SplashScreenDelay" value="3000" />
	<preference name="permissions"                value="none"/>
    <preference name="target-device"              value="universal"/>

</widget>